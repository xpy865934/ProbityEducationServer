<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
			    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
			    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xpy.domain.Products" table="products">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<version name="version"></version>
		<property name="product_name" column="product_name"></property>
		<property name="banji" column="banji"></property>
		<property name="author" column="author"></property>
		<property name="department" column="department"></property>
		<property name="product_path" column="product_path"></property>
		<property name="type" column="type"></property>
		<property name="tel" column="tel"></property>
	</class>
</hibernate-mapping>
			    